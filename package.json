{
  "name": "nextjs-bootstrap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "yarn build:client && yarn build:server",
    "build:client": "NODE_ENV=production next build",
    "build:server": "rm -rf dist && NODE_ENV=production tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node -r ./tsconfig-paths.config.js dist/server/index.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "test:unit": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:update-snapshot": "NODE_ENV=test jest --updateSnapshot",
    "test:coverage": "NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "1.0.0",
    "@apollo/client": "3.10.8",
    "@apollo/experimental-nextjs-app-support": "0.11.*",
    "@godaddy/terminus": "4.12.*",
    "@next/env": "13.5.*",
    "@next/third-parties": "14.2.*",
    "@tanstack/react-query": "5.0.*",
    "antd": "5.16.4",
    "autoprefixer": "10.4.19",
    "axios": "1.5.*",
    "cookie-parser": "1.4.*",
    "express": "4.18.*",
    "graphql": "16.8.*",
    "js-cookie": "3.0.*",
    "next": "14.2.2",
    "next-intl": "3.11.3",
    "react": "18.2.*",
    "react-dom": "18.2.*",
    "react-hook-form": "7.47.*",
    "tailwindcss": "3.3.*",
    "ua-parser-js": "0.7.*"
  },
  "devDependencies": {
    "@jest-mock/express": "2.0.*",
    "@storybook/addon-essentials": "8.0.*",
    "@storybook/addon-interactions": "8.0.*",
    "@storybook/addon-links": "8.0.*",
    "@storybook/blocks": "8.0.*",
    "@storybook/nextjs": "8.0.*",
    "@storybook/react": "8.0.*",
    "@storybook/testing-library": "0.2.*",
    "@svgr/webpack": "8.1.*",
    "@testing-library/jest-dom": "6.1.*",
    "@testing-library/react": "14.0.*",
    "@testing-library/user-event": "14.5.*",
    "@types/cookie-parser": "1.4.*",
    "@types/express": "4.17.*",
    "@types/jest": "29.5.*",
    "@types/node": "20.8.*",
    "@types/react": "18.2.*",
    "@types/react-dom": "18.2.*",
    "eslint": "8.52.*",
    "eslint-config-next": "13.5.*",
    "jest": "29.7.*",
    "jest-environment-jsdom": "29.7.*",
    "nodemon": "3.0.*",
    "storybook": "8.0.*",
    "ts-node": "10.9.*",
    "tsconfig-paths": "4.2.*",
    "typescript": "5.2.*"
  },
  "browserslist": [
    "Chrome >= 99",
    "iOS >= 15",
    "Firefox >= 115",
    "Edge >= 123",
    "last 2 versions",
    "not dead"
  ]
}
